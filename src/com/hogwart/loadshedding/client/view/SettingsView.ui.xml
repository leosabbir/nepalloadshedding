<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.important {
			font-weight: bold;
		}
	</ui:style>
	<g:HTMLPanel>
		<g:CheckBox ui:field="enableNotification"></g:CheckBox>
		<g:TextBox ui:field="notificationTime"></g:TextBox>
		<g:Label ui:field="errorLbl"></g:Label>
		<g:Button ui:field="confirm">OK</g:Button>
	</g:HTMLPanel>
</ui:UiBinder> 