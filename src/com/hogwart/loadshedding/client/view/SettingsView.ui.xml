<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.important {
			font-weight: bold;
		}
	</ui:style>
	<g:HTMLPanel styleName="settingsContainer">
		<div>
		<g:InlineLabel>Enable Notification</g:InlineLabel>
		<g:CheckBox ui:field="enableNotification"></g:CheckBox>
		</div>
		<div>
		<g:InlineLabel>Notification Time (0-60 min)</g:InlineLabel>
		<g:TextBox ui:field="notificationTime"></g:TextBox>
		</div>
		<div>
		<g:Label ui:field="errorLbl" visible="false" styleName="errorLabel"></g:Label>
		</div>
		<g:Button ui:field="confirm">Save</g:Button>
		
	</g:HTMLPanel>
</ui:UiBinder> 